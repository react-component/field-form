    "lib",
    "es",
    "dist",
    "assets/*.css"
  ],
  "license": "MIT",
  "main": "./lib/index",
  "module": "./es/index",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "compile": "father build",
