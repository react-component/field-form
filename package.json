{
  "name": "rc-field-form",
  "version": "2.4.0",
  "description": "React Form Component",
  "typings": "es/index.d.ts",
  "engines": {
    "node": ">=8.x"
  },
  "keywords": [
    "react",
    "react-component",
    "react-form",
    "form"
  ],
  "homepage": "https://github.com/react-component/field-form",
  "author": "smith3816@gmail.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-component/field-form.git"
  },
  "bugs": {
